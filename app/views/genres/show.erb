<p><h1><%=@genre.name%></h1></p>
<p><h2>Artists in this genre</h2></p>
<%@genre.artists.each do |artist|%>
    <p><h3><a href="/artists/<%=artist.slug%>"><%=artist[:name]%></a></h3></p>
<%end%>
<p><h2>Songs in this genre</h2></p>
<%@genre.songs.each do |song|%>
    <p><h3><a href="/songs/<%=song.slug%>"><%=song[:name]%></a></h3></p>
<%end%>